{
  "name": "unplugin-mp-verify-file-serve",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@10.18.3",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/hackycy/unplugin-mp-verify-file-serve#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackycy/unplugin-mp-verify-file-serve.git"
  },
  "bugs": {
    "url": "https://github.com/hackycy/unplugin-mp-verify-file-serve/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "transform"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "lint": "eslint .",
    "play:webpack": "npm -C playground/play-webpack run dev",
    "play:vite": "npm -C playground/play-vite run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^2.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@types/node": "^22.15.21",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "nodemon": "^3.1.10",
    "tsdown": "^0.12.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "webpack": "^5.99.9"
  }
}
